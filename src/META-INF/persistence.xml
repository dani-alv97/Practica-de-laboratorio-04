		<class>ec.edu.ups.entidades.Persona</class>
		<class>ec.edu.ups.entidades.Producto</class>
		<class>ec.edu.ups.entidades.Provincia</class>
		<class>ec.edu.ups.entidades.Stock</class>
		
		<properties>
            <property name="javax.persistence.schema-generation.database.action"
             
             value="none"/>
        </properties>
		
	</persistence-unit>
</persistence>
